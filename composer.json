{
  "name": "biiiiiigmonster/throttle",
  "type": "library",
  "keywords": [
    "swoole",
    "swoft",
    "swoft-component"
  ],
  "description": "throttle component for swoft framework",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "BiiiiiigMonster\\Throttle\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "BiiiiiigMonster\\ThrottleTest\\Unit\\": "test/unit/",
      "BiiiiiigMonster\\ThrottleTest\\Testing\\": "test/testing/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
